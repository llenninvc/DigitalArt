@model DigitalArt.Models.Arte

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>
@*función para eliminar un registro*@
<h3>Estas seguro de eliminar el registro?</h3>
<div>
    <h4>Arte</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreArte)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreArte)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DescripcionArte)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescripcionArte)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LinkArte)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LinkArte)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Img)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Img)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaRegistro)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdArte" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar a listar</a>
    </form>
</div>
