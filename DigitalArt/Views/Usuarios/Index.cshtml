@model IEnumerable<DigitalArt.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreUsuarios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionUsuarios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonoUsuarios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaRegistro)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreUsuarios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionUsuarios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoUsuarios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRegistro)
            </td>
            <td>
                <a asp-action="Edit" calss="btn btn-warning" asp-route-id="@item.IdUsuarios">Edit</a> |
                <a asp-action="Details" calss="btn btn-primary" asp-route-id="@item.IdUsuarios">Details</a> |
                <a asp-action="Delete" calss="btn btn-danger" asp-route-id="@item.IdUsuarios">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
