@model IEnumerable<DigitalArt.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Usuarios</h1>
<p>
    <a asp-action="Create" type="button"  class="neon-button neon-button__2">Create New</a>
</p>
<style>
    .card {
        display: flex;
        flex: 0 1 300px;
        flex-direction: column;
        border: 1px solid black;
        border-radius: 1rem;
        padding: 1rem;
        background: rgba(0, 0, 0, 0.7)url("https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/5d69d0da-04c9-4aed-a444-be1d11a96d69/dbnxexx-f586754e-7046-4e1d-b5f3-673126a58360.jpg/v1/fill/w_800,h_1132,q_75,strp/the_universe_below_by_aerroscape_dbnxexx-fullview.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..D2G7uZVJSSLW6M2d8HWoakM34_dASrkwb6wRbV2j7_E");
        height: 100%;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
        background-blend-mode: darken;
    }

</style>
<section>
@foreach (var item in Model)
{

    <div class="card">
        <div class="card-header">


    



        </div>
        <div class="card-body">

            <p class="card-text">
                    @Html.DisplayNameFor(model => model.NombreUsuarios) :
            </p>
            <p class="card-text">
                @Html.DisplayFor(modelItem => item.NombreUsuarios)
            </p>

            <p class="card-text">
                    @Html.DisplayNameFor(model => model.DireccionUsuarios) :
            </p>
            <p class="card-text">
                @Html.DisplayFor(modelItem => item.DireccionUsuarios)
            </p>

            <p class="card-text">
                @Html.DisplayNameFor(model => model.TelefonoUsuarios) :
            </p>
            <p class="card-text">
                @Html.DisplayFor(modelItem => item.TelefonoUsuarios)
            </p>

            <p class="card-text">
                @Html.DisplayNameFor(model => model.FechaRegistro) :
            </p>
            <p class="card-text">
                @Html.DisplayFor(modelItem => item.FechaRegistro)
            </p>
            <p class="card-text">
                    <a asp-action="Edit" type="button" class="btn btn-warning" asp-route-id="@item.IdUsuarios">Edit</a>
                    <a asp-action="Details" type="button" class="btn btn-primary" asp-route-id="@item.IdUsuarios">Details</a>
                    <a asp-action="Delete" type="button" class="btn btn-danger" asp-route-id="@item.IdUsuarios">Delete</a> 
            </p>


        </div>

            <img src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/62142078-a6c3-4fab-9f03-9ede1adbf7d1/de24m6w-7daa02b3-76aa-47fc-875c-c907753606d2.png/v1/crop/w_92,h_92,x_3,y_0,scl_0.10562571756602,strp/rin_by_dav_19_de24m6w-92s.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..tFsvUbXDPJFjtjx_W3bDoMkLV1QCZdRuBwjnTlCl6gU" alt="">
    </div>

}
    <br>
</section>
